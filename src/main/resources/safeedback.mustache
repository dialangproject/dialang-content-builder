<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
    <head>
        <meta http-equiv="content-type" content="text/html; charset=utf-8">
        <link rel="stylesheet" type="text/css" href="../../../../css/dialang.css" media="all"/>
        <script type="text/javascript" src="../../../../js/jquery-1.8.3.js"></script>
    </head>
    <body>
        <div id="container">
            <div id="topbar">
                <div id="navbar">
                    <button id="skipback" disabled><img src="../../../../images/skipb.gif"/></button>
                    <button id="back" title="{{prevtooltip}}"><img src="../../../../images/prev.gif"/></button>
                    <button id="next" disabled><img src="../../../../images/next.gif"/></button>
                    <button id="skipforward" disabled><img src="../../../../images/skipf.gif"/></button>
                    <button id="help"><img src="../../../../images/help.gif" height="23px;"/></button>
                </div>  <!-- /navbar -->
                <div id="logo"><img src="../../../../images/Logo.gif"/></div> 
            </div> <!-- /topbar -->
            <div id="content">
                <h1 id="welcome">{{{title}}}</h1>
                <p>{{{partOne}}}</p>
                <p>{{{partTwo}}}</p>
                <button id="about-sa-button">{{aboutSAText}}</button>
                <div class="bottom-margin"></div>
            </div> <!-- /content -->
            <div class="bottom-margin"></div>
        </div> <!-- /container -->
        <script type="text/javascript" src="../../../../js/dialang.js"></script>
        <script type="text/javascript" src="../../../../js/safeedback.js"></script>
    </body>
</html>
