<!DOCTYPE html>
<html lang="{{al}}">
  <head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <meta name="google" value="notranslate">
    <link rel="stylesheet" type="text/css" href="/prod/assets/css/dialang.css" media="all"/>
    <link rel="stylesheet" type="text/css" href="/prod/assets/css/smoothness/jquery-ui-1.9.2.custom.min.css" media="all"/>
    <link rel="stylesheet" type="text/css" href="/prod/assets/lib/keyboard/keyboard.min.css"/>
    <script src="/prod/assets/lib/jquery-1.11.3.min.js"></script>
    <script src="/prod/assets/lib/jquery-ui-1.9.2.custom.min.js"></script>
    <script src="/prod/assets/lib/jquery.form.js"></script>
    <script src="/prod/assets/lib/keyboard/jquery.keyboard.min.js"></script>
    <script src="/prod/assets/lib/keyboard/keyboard-layouts-combined.min.js"></script>
    <script src="/prod/assets/lib/mustache.js"></script>
    <script>
      var dialang = {
        state: '{{state}}',
        baseContentUrl: "{{baseContentUrl}}",
        flags: {
          disallowInstantFeedback: ('{{disallowInstantFeedback}}' === 'true') ? true : false,
          hideALS: ('{{hideALS}}' == 'true') ? true : false,
          hideTLS: ('{{hideTLS}}' == 'true') ? true : false,
          hideVSPT: ('{{hideVSPT}}' == 'true') ? true : false,
          hideVSPTResult: ('{{hideVSPTResult}}' == 'true') ? true : false,
          hideSA: ('{{hideSA}}' == 'true') ? true : false,
          hideTest: ('{{hideTest}}' == 'true') ? true : false,
          hideFeedbackMenu: ('{{hideFeedbackMenu}}' == 'true') ? true : false
        },
        session: {
          al: '{{al}}',
          tl: '{{tl}}',
          skill: '{{skill}}',
          vsptDone: {}
        },
        pass: {
          baskets: [],
          itemToBasketMap: {},
          items: [],
          subskills: {}
        }
      };
    </script>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-88708625-1', 'auto');
    </script>
  </head>
  <body>
    <div id="container">
      <div id="topbar">
        <div id="navbar">
          <button id="skipback"><img src="/prod/assets/images/skipb.gif"/></button>
          <button id="back"><img src="/prod/assets/images/prev.gif"/></button>
          <button id="next"><img src="/prod/assets/images/next.gif"/></button>
          <button id="skipforward"><img src="/prod/assets/images/skipf.gif"/></button>
          <button id="help"><img src="/prod/assets/images/help.gif" height="23px;"/></button>
          <button id="instantfeedback"><img src="/prod/assets/images/instantFeedbackOff.gif" height="23px;"/></button>
          <button id="keyboard-button"><img src="/prod/assets/images/keyb.gif" height="23px;"/></button>
          <button id="save-button"><img src="/prod/assets/images/savetodrive.png" height="23px;" disabled="disabled"/></button>
        </div>  <!-- /navbar -->
        <div id="logo">
          <div id="progressbar" class="dialang-progressbar"><div id="progressbar-label"></div></div>
          <img src="/prod/assets/images/Logo.gif"/>
        </div> <!-- /logo -->
      </div> <!-- /topbar -->

      <div id="help-dialog">
      </div> <!-- /help-dialog -->

      <div id="save-dialog">
      </div> <!-- /save-dialog -->

      <div id="load-dialog">
      </div> <!-- /load-dialog -->

      <div id="content">
      {{{content}}}
      </div> <!-- /content -->
      <div class="bottom-margin"></div>
    </div> <!-- /container -->
    <div id="keyboard-dialog">
      <div id="keyboard"></div>
    </div>

    <script src="/prod/assets/js/dialang.js"></script>
    <script src="/prod/assets/js/dialang_utils.js"></script>

    <script src="/prod/assets/js/navigation.js"></script>

  </body>
</html>
