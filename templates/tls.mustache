                <h1 id="tls-title">{{{tlsTitle}}}</h1>
                <table id="tls-table" cols="6">
                    <thead>
                        <tr>
                            <th></th>
                            <th><img src="/{{stage}}/assets/images/listening.gif" title="{{listeningTooltip}}"/></th>
                            <th><img src="/{{stage}}/assets/images/writing.gif" title="{{writingTooltip}}"/></th>
                            <th><img src="/{{stage}}/assets/images/reading.gif" title="{{readingTooltip}}"/></th>
                            <th><img src="/{{stage}}/assets/images/hammer.gif" title="{{structuresTooltip}}"/></th>
                            <th><img src="/{{stage}}/assets/images/book.gif" title="{{vocabularyTooltip}}"/></th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#testrows}}
                        <tr>
                            <td class="language-column">{{languageName}}</td>
                            <td class="language-skill-column"><a id="{{languageCode}}-listening" href="javascript:;" class="tls-link" title="{{languageName}}, {{listeningTooltip}}" tl="{{languageCode}}" skill="listening" calibrated="{{listeningcalibrated}}"><img src="/{{stage}}/assets/images/available.gif"/></a></td>
                            <td class="language-skill-column"><a id="{{languageCode}}-writing" href="javascript:;" class="tls-link" title="{{languageName}}, {{writingTooltip}}" tl="{{languageCode}}" skill="writing" calibrated="{{writingcalibrated}}"><img src="/{{stage}}/assets/images/available.gif"/></a></td>
                            <td class="language-skill-column"><a id="{{languageCode}}-reading" href="javascript:;" class="tls-link" title="{{languageName}}, {{readingTooltip}}" tl="{{languageCode}}" skill="reading" calibrated="{{readingcalibrated}}"><img src="/{{stage}}/assets/images/available.gif"/></a></td>
                            <td class="language-skill-column"><a id="{{languageCode}}-structures" href="javascript:;" class="tls-link" title="{{languageName}}, {{structuresTooltip}}" tl="{{languageCode}}" skill="structures" calibrated="{{structurescalibrated}}"><img src="/{{stage}}/assets/images/available.gif"/></a></td>
                            <td class="language-skill-column"><a id="{{languageCode}}-vocabulary" href="javascript:;" class="tls-link" title="{{languageName}}, {{vocabularyTooltip}}" tl="{{languageCode}}" skill="vocabulary" calibrated="{{vocabularycalibrated}}"><img src="/{{stage}}/assets/images/available.gif"/></a></td>
                        </tr>
                        {{/testrows}}
                    </tbody>
                </table>

                <div id="tls-legend">
                    <img src="/{{stage}}/assets/images/done.gif"/>{{done}}
                    <img src="/{{stage}}/assets/images/available.gif"/>{{available}}
                    <img src="/{{stage}}/assets/images/notavailable.gif"/>{{notavailable}}
                </div>

                <div id="disclaimer-dialog" title="{{disclaimerTitle}}">
                    <div class="dialang-dialog-content">
                        <div id="disclaimer-text">{{{disclaimer}}}</div>
                        <div class="dialog-button-panel"><button id="disclaimer-button"><img src="/{{stage}}/assets/images/yes.gif"/></button><br />{{ok}}</div>
                    </div>
                </div> <!-- /disclaimer-dialog -->

                <div id="confirm-dialog" title="{{confirmationTitle}}" class="dialang-dialog">
                    <div class="dialang-dialog-content">
                        <p>{{testChosen}}</p>
                        <p id="confirmation_langskill"></p>
                        <div id="confirmation-button-table">
                            <div>
                                <div align="center"><button id="confirm-yes"><img src="/{{stage}}/assets/images/yes.gif"/></button></div>
                                <div align="center"><button id="confirm-no"><img src="/{{stage}}/assets/images/no.gif" width="58" height="58"/></button></div>
                            </div>
                            <div>
                                <div align="center">{{yes}}</div><div align="center">{{no}}</div>
                            </div>
                        </div>
                    </div>
                </div> <!-- /confirmation-dialog -->
